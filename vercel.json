{
  "version": 2,
  "name": "nanoai-tts",
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python@3.0.0",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1",
    "ENVIRONMENT": "vercel"
  }
}
